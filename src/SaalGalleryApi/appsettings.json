{
  "ConnectionStrings": {
    "RedisSaalDB": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore.Mvc.Internal": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Trace",
      "Microsoft.AspNetCore.Authentication": "Information"
    },
    "DbLogger": {
      "LogLevel": {
        "Default": "Information",
        "System.Net.Http.HttpClient": "Warning",
        "Microsoft": "Warning"
      }
    }
  },
  "OtelSettings": {
    "ServiceName": "verification-engine",
    "ServiceVersion": "1.0",
    "ServiceEnvironment": "staging",
    "OtelEndpointUrl": "https://localhost:7026",
    "EnablePrintDbStatements": true,
    "EnableShowMetrics": false,
    "OutputTracesToConsole": true,
    "EnableOtelLogging": true,
    "OutputOtelLogsToConsole": true
  },
  "CustomAppSettings": {
    "JwtConfig": {
      "ValidIssuer": "https://localhost/",
      "ValidAudience": "https://localhost/"
    }
  },
  "AllowedHosts": "*"
}
